# ========================================
# Application Configuration
# ========================================
VITE_APP_NAME=PropManager
VITE_APP_VERSION=1.0.0
VITE_ENV=development

# ========================================
# API Configuration
# ========================================
# Backend API endpoint (use Google Cloud Run URL in production)
VITE_API_URL=http://localhost:3001/api
VITE_API_TIMEOUT=30000

# ========================================
# Google Cloud Platform (Production)
# ========================================
# Project Configuration
VITE_GCP_PROJECT_ID=your-project-id
VITE_GCP_REGION=us-central1

# Cloud Storage (for file uploads)
VITE_GCS_BUCKET_NAME=propmanager-uploads
VITE_GCS_PUBLIC_URL=https://storage.googleapis.com

# Firebase Authentication (optional)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# ========================================
# Feature Flags
# ========================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_MOCK_DATA=true

# ========================================
# External Services
# ========================================
# Payment Processing
VITE_STRIPE_PUBLIC_KEY=
VITE_MERCADOPAGO_PUBLIC_KEY=

# Maps and Location
VITE_GOOGLE_MAPS_API_KEY=

# Monitoring and Error Tracking
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=

# ========================================
# Security
# ========================================
# JWT Configuration (backend should manage this)
# Session timeout in milliseconds (1 hour = 3600000)
VITE_SESSION_TIMEOUT=3600000

# CORS allowed origins (comma-separated, backend config)
# VITE_CORS_ORIGINS=https://propmanager.app,https://www.propmanager.app
